package itWorkInfoTech;

import java.util.Iterator;

public class PrimeNumber {
	public static void main(String[] args) {
		
	
		
		for(int j=0;j<20;j++)
		{
			int n=j;
			int count=0;
			for (int i = 0; i < n; i++) {
				if (n%2==0) {
					count++;
				}
			}
			if (count==2) {
				System.out.println(n);
			}
		}
}
}